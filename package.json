    "scripts": {
        "build": "nest build",
        "lint": "eslint . --ext .ts",
        "dev": "npx prisma generate && npx prisma migrate deploy && nest start --watch",
        "start:prod": "node dist/main",
        "start": "npx prisma generate && npx prisma migrate deploy && node dist/main",
        "start:docker": "prisma generate && prisma migrate deploy && node dist/main",
        "generate:enum": "tsx src/scripts/generate.enum.ts",
        "prisma:validate": "npx prisma validate",
        "prisma:format": "npx prisma format",
        "prisma:generate": "npm run generate:enum && npx prisma generate",
        "prisma:push": "npx prisma db push",
        "prisma:migrate": "npx prisma migrate deploy",
        "prisma:reset": "npx prisma migrate reset",
        "prisma:seed": "tsx prisma/seed.ts",
        "prisma:studio": "npx prisma studio",
        "format": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
        "format:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "lint:fix": "eslint . --ext .ts,.js,.jsx,.tsx --fix",
        "ci:check": "npm run format && npm run lint && npm run prisma:validate && npm run build && npm run start:prod",
        "deploy:check": "docker compsoe --profile prod build --no-cache",
        "ci:fix": "npm run format:fix",
        "prepare": "husky"
    },
